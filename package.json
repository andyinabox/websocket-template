{
  "name": "simple-websockets",
  "version": "0.0.1",
  "description": "A simple template for prototyping with browserify/express/websockets",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently -k \"npm run server\" \"npm run browser\"",
    "browser": "watchify browser.js -v -t babelify -o public/bundle.js -p errorify",
    "server": "node server.js",
    "globals": "npm install -g browserify && npm install -g watchify"
  },
  "author": {
    "name": "Andy Dayton",
    "email": "amdayton@gmail.com",
    "url": "http://andydayton.com/"
  },
  "dependencies": {
    "bufferutil": "^1.3.0",
    "express": "^4.14.0",
    "utf-8-validate": "^2.0.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "concurrently": "^3.1.0",
    "errorify": "^0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/andyinabox/simple-websockets.git"
  },
  "homepage": "https://github.com/andyinabox/simple-websockets"
}
